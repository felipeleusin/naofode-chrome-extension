{
  "manifest_version": 2,
  "name": "Naofo.de Button",
  "description": "Extensão para agilizar utilização do serviço http://naofo.de",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon38.png",
    "default_title": "Criar naofo.de"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "38": "icon38.png",
    "128": "icon128.png"
  }
}
